        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.welcome.WelcomeActivitye"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_welcome_activitye"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".ui.activity.login.LoginActivity" />
        <activity android:name=".ui.activity.viewpager.ViewpagerShowActivity" />
        <activity android:name=".ui.activity.viewpager.DemoFragmentActivity" />
        <activity
            android:name=".ui.activity.tab.TabActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".ui.activity.permissionmodel.PermissionModelActivitye" />
        <activity android:name=".ui.activity.contract.ContractActivity" />
