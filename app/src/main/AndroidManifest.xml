        <activity android:name=".ui.activity.qr.QRActivity" />
        <activity android:name=".ui.activity.qr.QrMangerActivity" />
        <activity android:name=".ui.activity.msg.MsgActivity" />
        <activity
            android:name=".ui.activity.friendcircle.FriendCircleActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".ui.activity.cache.CacheActivity" />
        <activity
            android:name=".ui.activity.dayornight.DayOrNightActivity"
            android:theme="@style/AppDayTheme" />
        <activity android:name=".ui.activity.webview.WebviewActivity" />
        <activity android:name=".ui.activity.other.V4V7DesignActivity" />
        <activity
            android:name=".ui.activity.other.toolbar.ToolBarActivity"
            android:label="@string/title_activity_tool_bar" />
        <activity
            android:name=".ui.activity.other.toolbatlayout.ToolbarLayoutActivity"
            android:theme="@style/ColorTranslucentTheme" />
        <activity android:name=".ui.activity.other.custombehavior.CustomeBehaviorActivity" />
        <activity android:name=".ui.activity.blur.BlurActivity" />
        <activity android:name=".ui.activity.crash.CrashActivity" />
        <activity android:name=".ui.activity.retrofit.RetrofitActivit"></activity>
